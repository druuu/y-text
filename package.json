{
  "name": "y-text",
  "version": "9.5.1",
  "description": "Text Type for Yjs",
  "main": "./y-text.node.js",
  "browser": "./y-text.js",
  "module": "./src/y-text.js",
  "scripts": {
    "test": "npm run lint",
    "dist": "rollup -c rollup.browser.js && rollup -c rollup.node.js",
    "lint": "standard",
    "watch": "concurrently 'rollup -wc rollup.browser.js' 'rollup -wc rollup.node.js'",
    "postversion": "npm run dist",
    "postpublish": "tag-dist-files --overwrite-existing-tag"
  },
  "files": [
    "y-text.*"
  ],
  "standard": {
    "ignore": [
      "/y-text.*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/y-js/y-text"
  },
  "keywords": [
    "Yjs",
    "OT",
    "Collaboration",
    "Synchronization",
    "ShareJS",
    "Coweb",
    "Concurrency"
  ],
  "author": "Kevin Jahns <kevin.jahns@rwth-aachen.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/y-js/y-text/issues"
  },
  "homepage": "http://y-js.org",
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-latest": "^6.24.1",
    "chance": "^1.0.10",
    "concurrently": "^3.4.0",
    "cutest": "^0.1.9",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-multi-entry": "^2.0.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^1.0.2",
    "rollup-watch": "^3.2.2",
    "standard": "^10.0.2",
    "tag-dist-files": "^0.1.6"
  },
  "peerDependencies": {
    "yjs": "^13.0.0-17",
    "y-array": "^11.0.0-4"
  },
  "dependencies": {
    "fast-diff": "^1.1.1"
  }
}
